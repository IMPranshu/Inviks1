<resources>

    <!-- Base application theme. -->
    <!--<style name="AppTheme" parent="@android:style/Theme.Holo.Light">-->
    <style name="AppTheme" parent="@style/Theme.AppCompat.Light">
        <item name="android:actionBarStyle">@style/MyActionBar</item>
        <item name="actionBarStyle">@style/MyActionBar</item>
        <item name="android:imageButtonStyle">@style/imgButton</item>
        <item name="android:homeAsUpIndicator">@drawable/ic_action_back</item>
    </style>
<style name="simpleButton" parent="@android:style/Widget.Button">
    <item name="android:background">@drawable/homepagebutton</item>
</style>
    <style name="hyperLink" parent="Base.Widget.AppCompat.Button.Borderless">
        <item name="android:textAllCaps">false</item>
        <item name="android:textColor">@color/hyperLinkColor</item>
    </style>
    <style name="imgButton" parent="@android:style/Widget.Button">
        <item name="android:background">@drawable/image_button_global</item>

    </style>
    <style name="MyActionBar" parent="@style/Widget.AppCompat.Light.ActionBar.Solid.Inverse">
        <item name="android:background">@color/mainColor</item>
        <item name="background">@color/mainColor</item>
        <!--<item name="android:displayOptions">showHome|homeAsUp|showTitle</item>
        <item name="displayOptions">showHome|homeAsUp|showTitle</item>-->
        <item name="android:icon">@android:color/transparent</item>
        <!--<item name="android:logo">@drawable/ic_action_threebar</item>-->
    </style>
    <style name="HomePageButtons" parent="">
        <item name="android:background">#FFFFFF</item>

    </style>
    <color name="mainColor">#92D050</color>
    <color name="transparent">#00000000</color>
    <color name="hyperLinkColor">#0000FF</color>
</resources>

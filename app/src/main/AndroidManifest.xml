<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.inviks.www.inviks1" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:logo="@drawable/ic_action_threebar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SearchActivity"
            android:label="@string/title_activity_search" >
        </activity>
        <activity
            android:name=".ItemDetails"
            android:label="@string/title_activity_item_details" >
        </activity>
        <activity
            android:name=".LoginMain"
            android:label="@string/title_activity_login_main" >
        </activity>
        <activity
            android:name=".CartView"
            android:label="@string/title_activity_cart_view" >
        </activity>
    </application>

</manifest>
